{
    "conponentDefineType": "helm/service",
    "name": "ontology-manager",
    "version": "6.0.0",
    "configSchema": {
        "title": "ontology-manager",
        "description": "组件配置定义",
        "type": "object",
        "properties": {
            "resources": {
                "type": "object",
                "title": "容器资源控制",
                "properties": {
                    "requests": {
                        "type": "object",
                        "description": "容器运行需要的最小资源分配",
                        "properties": {
                            "memory": {
                                "type": "string",
                                "title": "内存"
                            },
                            "cpu": {
                                "type": "string",
                                "title": "cpu"
                            }
                        }
                    },
                    "limits": {
                        "type": "object",
                        "description": "容器运行最大资源限制",
                        "properties": {
                            "memory": {
                                "type": "string",
                                "title": "内存"
                            },
                            "cpu": {
                                "type": "string",
                                "title": "cpu"
                            }
                        }
                    }
                }
            },
            "replicaCount": {
                "type": "number",
                "title": "服务副本数"
            }
        }
    },
    "dependence": [
        {
            "name": "rds"
        },
        {
            "name": "opensearch"
        },
        {
            "name": "class-443"
        }
    ]
}